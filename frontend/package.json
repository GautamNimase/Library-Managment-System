{
  "name": "library-management-system-frontend",
  "version": "1.0.0",
  "description": "Frontend for Library Management System",
  "main": "index.html",
  "scripts": {
    "build": "echo 'Static frontend - no build step required'",
    "start": "serve . -p 3000",
    "dev": "serve . -p 3000",
    "serve-open": "serve . -p 3000 && start http://localhost:3000",
    "http-server": "node --no-deprecation ./node_modules/.bin/http-server . -p 3000 -o",
    "http-server-dev": "node --no-deprecation ./node_modules/.bin/http-server . -p 3000 -o -c-1"
  },
  "keywords": [
    "library",
    "management",
    "system"
  ],
  "author": "Library Management System",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^14.1.1",
    "serve": "^14.2.5"
  }
}
